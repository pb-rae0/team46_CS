module compare (
    input z,
    input v,
    input n,
    input alufn_signal[6],
    output cmp
) {
    // mux_4 mux;
    
    always {
        case(alufn_signal[2:1]){
            b01: cmp = z
            b10: cmp = n ^ v
            b11: cmp = z | (n^v)
            default: cmp = 0
            
        }
    }
}